'use strict';

const {echo, end, $_GET} = require('../../../index');

const users = [
  {
    userId : 1001,
    name : 'Lily',
    age : 21
  },
  {
    userId : 1002,
    name : 'Lucy',
    age : 23
  },
  {
    userId : 1003,
    name : 'Bruce',
    age : 22
  }
];

function getUser(id) {
  let user = {};
  for (var i in users) {
    if (users[i].userId == id) {
      user = users[i];
      break;
    }
  }
  return user;
}

let user = getUser($_GET['userId']);

echo(user);
end(); // 结束脚本